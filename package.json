      {
        "language": "c",
        "path": "./snippets/lch.json"
      },
      {
        "language": "c",
        "path": "./snippets/lch_include.json"
      },
      {
        "language": "c",
        "path": "./snippets/lc_str.json"
      }
    ]
  }
}
